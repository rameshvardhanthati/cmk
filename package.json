{
  "name": "ChangeMaker",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^5",
    "@sap/hana-client": "^2.10.20",
    "express": "^4",
    "hdb": "^0.18.3"
  },
  "scripts": {
    "start": "cds run",
    "undeploy": "cf undeploy ChangeMaker --delete-services --delete-service-keys"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "build": {
      "target": "."
    },
    "hana": {
      "deploy-format": "hdbtable"
    },
    "requires": {
      "db": {
        "kind": "hana"
      }
    }
  },
  "devDependencies": {
    "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
    "@sap/hdi-deploy": "4.2.3",
    "@sap/ux-specification": "^1.90.16",
    "@sap/ux-ui5-tooling": "^1.4.4",
    "@sapui5/ts-types": "^1.92.2",
    "eslint": "^7.32.0"
  }
}