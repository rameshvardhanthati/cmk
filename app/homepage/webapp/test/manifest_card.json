{
	"sap.app": {
		"id": "my.new.card",
		"type": "card",
		"applicationVersion": {
		  "version": "1.0.0"
		}
	},
	"sap.card": {
        "type": "List",
        "header": {
			"title": "OCM References",
            "actions": [
				{
					"type": "Navigation",
					"url": "https://www.goodreads.com/"
				}
            ]
            
        },
        "content": {

           
			"data": {
				"json": [
					{
						"Name": "Enablement",
                        "url": "https://im.enter.sap/sap/ino/#/idea/61289",
                        "icon": "sap-icon://education"
					},
					{
						"Name": "Glossary",
						"url": "https://im.enter.sap/sap/ino/#/idea/61289",
                        "icon": "sap-icon://alphabetical-order"
					},
					{
						"Name": "Wiki",
						"url": "https://im.enter.sap/sap/ino/#/idea/61289",
                        "icon": "sap-icon://inspection"
					}
				]
			},
			"item": {
                "icon": {
                    "src": "{icon}"
                },
				"title": {
					"value": "{Name}"
				},
                "actions": [
                    {
                        "type": "Navigation",
						"enabled": "{= ${url}}",
						"parameters": {
							"url": "{url}"
						}
                        
                    }
                ]
            }
        }
	}
}